<?xml version="1.0"?>
<package >
  <metadata>
    <id>confudge</id>
    <version>0.2</version>
    <title>confudge</title>
    <authors>khanage</authors>
    <owners>khanage</owners>
    <description>
      Confudge makes it easy to use typed configuration without the overhead of creating adapter objects. It uses a convention and some overridable defaults to
      quickly enable typed configuration in .net.
    </description>
    <releaseNotes>
      * Add default parsers
      * Add recursive parsing (sub objects will be mapped)
    </releaseNotes>
    <summary>
      Confudge is a configuration helper turning normal .net config into typed values.
    </summary>
    <language>en-US</language>
    <projectUrl>https://github.com/khanage/confudge</projectUrl>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>
    <copyright>Copyright  2014</copyright>
    <dependencies>
      <dependency id="bearded.monads" version="1.0.12" />
    </dependencies>
  </metadata>
  <files>
    <file src="lib\" target="lib" />
  </files>
</package>